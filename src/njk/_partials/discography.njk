<div class="discography">
  <div class="row">
        {% for disc in queen.discography%}
          <div class="col-sm-3 col-lg-1">
            <img src="img/discs/{{ disc.image }}" class="img-fluid"  alt="{{ disc.title }}">
              <h5 class="mt-0 mb-1">{{ disc.title }}</h5>
              <h6> {{ disc.year }} </h6>
          </div>
        {% endfor %}
  </div>
</div>